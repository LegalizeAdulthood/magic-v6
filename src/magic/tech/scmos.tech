	shrink 300
	calma 57 1
    templayer TNF
	bloat-or nffet,enfet,nfet ndiff 450 poly 300
    templayer TPF
	bloat-or pffet,epfet,pfet pdiff 450 poly 300
    layer CSP
	bloat-or allPDiff * 300 nsd,nsc/active 0
	bloat-or pfet,pffet,epfet * 300 pdiff 450 nsd,nsc/active 0
	bloat-or psc/active,psd * 300 ndiff,ndc/active,nfet 0
