/* -------------------------------------------------------------------- *
 * scmos.tech --scalable CMOS                                           *
 *                                                                      *
 * Defines the MOSIS 1.2/1.6/2.0/3.0 micron Scalable CMOS technology    *
 * (up to revision #7)                                                  *
 * Some of the characteristics of this technology are:                  *
 *                                                                      *
 * 1. 2 levels of metal.                                                *
 *    2 levels of poly                                                  *
 *      (second poly is used for capacitor or electrode fet (efet)      *
 *    low noise anaolog layers: bd, nbd, nbdc, emc, clc, pbase,pbc 	*
 * 2. No stacked contacts (all contacts are to 1st-level metal).        *
 * 3. Wells can be implicit or explicit in the technology,              *
 *    and both types of of diffusion must have well contacts.           *
 * 4. Painting well over p-type diffusion will result in N type diff    *
 * 5. Scalable with Default to be 2.0 micro rules                       *
 * 6. Substrate contacts must be 3 units away from gates                *
 *                                                                      *
 * Revision 6.0.1 (sllu)                                                *
 * 	added CX for collector layer					*
 * Revised for Magic Version 6.						*
 * Revision 6.0 90/05/11 20:12:34 pi					*
 * include color versatech support					*
 * eliminated active2 plane						*
 * (rule version # 5.01 (S. Lu) = rule 5.0 + mod. for cc spacing	*
 * (rule version # 5.0 (Shih-Lien Lu sllu@MOSIS.EDU) 8/15/89)		*
 * (rule 5.0 = rule 4.01 + new layers for analog process		*
 * (rule 4.01 = rule 4.0 + comments + cifout style lambda=0.8(pwell)	*
 * 	(correction made by L. McMurchie of UW)				*
 * (rule 4.0 = rule 3.1 + new layer : electrode)                        *
 * (rule 3.1 = rule 3.0 + new cifout  method for select layers )        *
 *      (design can be more compact now with this version )             *
 *      (layout should be upward compatible:: you old layout will not)  *
 *      (flag any drc violations)                                       *
 * (rule 3.0 = rule 2.0 + rev #6 + new cifin section for new nwell)     *
 *                                                                      *
 * (rule version # 2.0 10/28/87)                                        *
 * (rule 2.0 = rule 1.9 + modification to extract section               *
 * (rule 1.9 = rule 1.82 + additions of two more CIF in/output styles   *
 * (rule 1.82 = rule 1.81+ modification of drc #4.1 & cifoutput of wells*
 * (rule 1.81 = rule 1.8 + modification on line 1761                    *

 * (rule 1.8 = rule 1.7 + Rev 5 of the SCMOS rules)                     *
 *   (difference from rule 1.7:                                         *
 *      (1) well width = 9 lambda                                       *
 *      (2) N well process accepts both N and P selects                 *
 * ---------------------------------------------------------------------*/

tech
    scmos
end

planes
    active,diffusion,polysilicon,act,a
    metal1,m1
    metal2,m2
    well,w
    oxide,ox
end

types

    /* primary layers -12 */

    active	polysilicon,red,poly,p
    active	poly2,electrode,el,p2
    active	ndiffusion,green,ndiff
    active	pdiffusion,brown,pdiff
    metal1	metal1,m1,blue
    metal2	metal2,m2,purple
/* cannot do it for magic version 6
   magic 4.10
    metal2	viahole,viah,vh
*/
    well	pwell,pw
    well	nwell,nw
    well	fpabse,fpb
    active	bccdiffusion,bd
    active	nbccdiffusion,nbd
    active	pbase,pb

    /* Contacts between interconnection layers -9 */

    active	polycontact,pcontact,polycut,pc
    active	ndcontact,ndiffcut,ndc
    active	pdcontact,pdiffcut,pdc
    active	capcontact,ccontact,capc,cc
    active	electrodecontact,econtact,ec,poly2contact,p2c
    active	pbasecontact,pbcontact,pbc
    metal1	m2contact,m2cut,m2c,via,v
    active	nbccdiffcontact,nbdc

    /* Transistors -10*/

    active	ntransistor,nfet
    active	ptransistor,pfet
    active	entransistor,enfet
    active	eptransistor,epfet
    active	doublentransistor,nfloating-gate,nfloatg,nfg,nffet
    active	doubleptransistor,pfloating-gate,pfloatg,pfg,pffet
    active	collectorcontact,colcontact,colc,coc,clc
    active	collector,coll,col,co,cl
    active	emitter,emit,em
    active	emittercontact,emitcontact,emc

    /* capacitors -2*/

    active	capacitor,polycap,pcap,cap

    /* Well contacts -4*/
    /* pohmic and nohmic are included for compatibility */
    /* nwc, pwc, etc ... are included for compatibility */

    active	psubstratepcontact,ppcontact,ppcont,psc,ppc,pwc,pwcontact,wellplug
    active	nsubstratencontact,nncontact,nncont,nsc,nnc,nwc,nwcontact,subplug
    active	psubstratepdiff,psd,ppdiff,ppd,pohmic
    active	nsubstratendiff,nsd,nndiff,nnd,nohmic

    /* Additional stuff, used in pads. -2*/
 
    metal2	pad
    oxide	glass
end

contact
    clc		col	metal1
    emc		emit	metal1
    pbc		pbase	metal1
    nbdc	nbd	metal1
    ec		poly2	metal1
    cc		cap	metal1
    pc		poly	metal1
    ndc		ndiff	metal1
    pdc		pdiff	metal1
    ppc		ppdiff	metal1
    nnc		nndiff	metal1
    m2c		metal2	metal1
    pad		metal2	metal1
/*
 magic 4.10
    vh		metal2	metal1
*/
end

styles
    styletype mos

    poly	1
    poly2	14
    ndiff	2
    pdiff	4
    nfet	6
    nfet	7
    pfet	8
    pfet	9
    enfet	6
    enfet	14
    epfet	8
    epfet	14
    nffet	6
    nffet	7
    nffet	14
    pffet	8
    pffet	9
    pffet	14
    pbase	15
    pbc		15
    pbc		20
    pbc		32
    emit	16
    emc		16
    emc		20
    emc		32
    col		3
    clc		3
    clc		20
    clc		32
    fpb		17
    bccd	17
    cap		1
    cap		14
    cc		1
    cc		14
    cc		20
    cc		32
    metal1	20
    metal2	21
    pcontact	1
    pcontact	20
    pcontact	32
    ec   	14
    ec   	20
    ec   	32
    ndcontact	2
    ndcontact	20
    ndcontact	32
    pdcontact	4
    pdcontact	20
    pdcontact	32
/*  magic4.10
    vh		21
    vh		32
*/
    m2contact	20
    m2contact	21
    m2contact	33
    ppcontact	5
    ppcontact	20
    ppcontact	32
    nncontact	3
    nncontact	20
    nncontact	32
    psd		5
    nsd		3
    nwell	12
    pwell	13
    pad		20
    pad		21
    pad		33
    pad		34
    bd		17
    nbd		17
    nbd		 3
    nbdc	 3
    nbdc	17
    nbdc	20
    nbdc	32
    glass	34
    error_p	42
    error_s	42
    error_ps	42
end

compose
    compose	nfet	poly	ndiff
    compose	pfet	poly	pdiff
    compose	enfet	poly2	ndiff
    compose	epfet	poly2	pdiff
    compose	pffet   pfet    poly2
    compose	nffet   nfet    poly2
    compose	pffet   epfet   poly
    compose	nffet   enfet   poly
    /* The rules below allow nwell to be painted over an area to
     * flip all the p-well types to n-well types.  Pwell can be
     * painted to flip in the reverse.  */
/* magic4.10
    paint	m2	vh	vh
    paint	vh	m2	vh
*/
    paint	clc	col	clc
    paint	emc	emit	emc
    paint	emc	pbase	emc
    paint	poly2	poly	cap
    paint	poly	poly2	cap
    paint	poly	cap	cap
    paint	poly2	cap	cap
    paint	cap	poly	cap
    paint	cap	poly2	cap
    paint	poly	ec	cc
    paint	ec	poly	cc
    paint	pwell   nwell   nwell
    paint       nwell   pwell   pwell
    paint	pdc/active	pwell	ndc/active
    paint	pdc/m1	pwell	ndc/m1
    paint	pfet	pwell	nfet
    paint	epfet	pwell	enfet
    paint	pdiff	pwell	ndiff
    paint	nsd	pwell	psd
    paint	nsc/active	pwell	psc/active
    paint	ndc/active	nwell	pdc/active
/* you need the following lines for magic 4.10
    paint	nsc/m1e	pwell	psc/m1e
    paint	ndc/m1	nwell	pdc/m1
    paint	psc/m1	nwell	nsc/m1
    paint	pdc/m1	pwell	nsc/m1
*/
    paint	nfet	nwell	pfet
    paint	enfet	nwell	epfet
    paint	ndiff	nwell	pdiff
    paint	psd	nwell	nsd
    paint	psc/active	nwell	nsc/active
end

#define DiffCut pdc/act,ndc/act,psc/act,nsc/act
/*
#define newCut ec/m1,cc/m1,clc/m1,emc/m1,pbc/m1
#define allMetal1 m1,m2c/m1,pc/m1,ndc/m1,pdc/m1,psc/m1,nsc/m1,pad/m1,newCut
*/
/* for magic 4.10 replace these two lines from above two lines */
#define newCut ec,cc,clc,emc,pbc
#define allMetal1 m1,m2c/m1,pc,ndc,pdc,psc,nsc,pad,newCut
#define allNDiff ndiff,ndc/act,nfet,enfet,nffet
#define allPDiff pdiff,pdc/act,pfet,epfet,pffet
#define allOhmics psd,psc/act,nsd,nsc/act
#define allBiDiff pbase,pbc,emit,emc/act,col,clc/act
#define allDiff allNDiff,allPDiff,allOhmics,allBiDiff
#define allPoly poly,pc/active,nfet,pfet,cap,cc,nffet,pffet
#define allPolyP poly,pc/active,nfet,pfet
#define allPoly2 poly2,ec/act,cap,enfet,epfet,capc/act,nffet,pffet
#define allPoly2P poly2,ec/act,enfet,epfet
/* magic 4.10
#define allMetal2 m2,m2c/m2,pad/m2,vh/m2
*/
#define allMetal2 m2,m2c/m2,pad/m2
#define allFet nfet,pfet,enfet,epfet,pffet,nffet

connect
    allMetal2		allMetal2
    allMetal1	 	allMetal1
    poly,pc/active,nfet,pfet            poly,pc/active,nfet,pfet
    allPoly2		allPoly2
    ndiff		ndc
    pdiff		pdc
    nbd			nbdc
    nwell,nnc,nsd,col,clc	nwell,nnc,nsd,clc,col
    pwell,ppc,psd	pwell,ppc,psd
    pbase,pbc,fpb	pbase,pbc,fpb
end

/* WARNING ::::: automatic generation of wells does not guarantee   */
/* rules on width and spacing of wells are followed !!              */
/* It is strongly recommanded that designers layout their own wells */

/* PWELL styles cannot generate CBA and CCD correctly */
/* BOTH NWELL and GEN can do CCD and CBA */

cifoutput
/* default: fab on 2.0 micron (NW) rules each magic unit is 100 centmicrons */

style lambda=1.0(nwell)
    scalefactor 100 50
    layer CWN nwell
	bloat-or pbase * 600
	bloat-or fpb * 600
	bloat-or pdiff,pdc,pfet * 500
	bloat-or nsc,nnd * 300
	bloat-or clc * 100
	grow 300
	shrink 300
	calma 42 1
    layer CMS allMetal2
	labels m2
	calma 51 1
    layer CMF pad
	grow 100
	or allMetal1
	labels allMetal1
	calma 49 1
    layer CPG cap,cc
	or allPoly
	labels poly,nfet,pfet
	calma 46 1
    layer CAA clc
	grow 100
	or allDiff,nbdc,nbd,bd
	labels ndiff,pdiff
	calma 43 1
    layer CX col,clc
	grow 100
	calma 60 1
    layer CVA pad
	shrink 500
	calma 50 1
    layer CVA m2c
	squares 100 200 300
/* for magic 4.10
	or vh
*/
	calma 50 1
    layer CEL allPoly2
	calma 56 1
    layer CCE cc
	squares 200
	calma 55 1
    layer CCE ec
	squares 200
	calma 55 1
    layer CCA ndc,pdc,emc,pbc,clc,nbdc
	squares 200
	calma 48 1
    layer CCA nncont,ppcont
	squares 200
	calma 48 1
    layer CCP pc
	squares 200
	calma 47 1
    layer CBA emc,emit
	grow 400
	bloat-or pbc * 100
	or pb,fpb
	grow 200
	shrink 200
	calma 57 1
    templayer TNF
	bloat-or nffet,enfet,nfet ndiff 300 poly 200
    templayer TPF
	bloat-or pffet,epfet,pfet pdiff 300 poly 200
    layer CSN clc
	grow 300
    layer CSN emc,emit,col
	grow 200
	bloat-or allNDiff * 200 psd,psc/active 0
	bloat-or nfet,nffet,enfet * 200 ndiff 300 psd,psc/active 0
	bloat-or nsc/act,nsd * 200 pdiff,pdc/active,pfet 0
	bloat-or nbd,nbdc * 200 bd 0
	and-not TPF
	grow 100
	shrink 100
	shrink 50
	grow 50
	calma 45 1
    layer CSP pbc
	grow 100
	bloat-or allPDiff * 200 nsd,nsc/active 0
	bloat-or pfet,pffet,epfet * 200 pdiff 300 nsd,nsc/active 0
	bloat-or psc/active,psd * 200 ndiff,ndc/active,nfet 0
	and-not TNF
	and-not CSN
	grow 100
	shrink 100
	shrink 50
	grow 50
	calma 44 1
    layer CCD bd,nbd,nbdc
	grow 200
	calma 58 1
    layer COG pad
	shrink 600
	or glass
	calma 52 1
    layer XP pad

/* technology SCP */
style lambda=1.5(pwell)
    scalefactor 150 25
    layer CWP pwell
	bloat-or ndiff,ndc,nfet * 750
        bloat-or psc,ppd * 450
/* merge wells of the same potential only */
/* if wells are painted explicitly, they are check with a spacing of 9
 in drc section */
