    layer CEL allPoly2
	calma 56 1
    layer CCE ec
	squares 300
	calma 55 1
    layer CCE cc
	squares 300
	calma 55 1
    layer CCA ndc,pdc,emc,pbc,clc
	squares 300
	calma 48 1
    layer CCA nncont,ppcont
	squares 300
	calma 48 1
    layer CCP pc
	squares 300
	calma 47 1
    layer CBA emc,emit
	grow 600
	bloat-or pbc * 150
	or pb,fpb
