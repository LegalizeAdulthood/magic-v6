/* scmos rules rev 7 */
drc
    width pwell 10 \
	"P-Well width must be at least 10 (MOSIS rule #1.1)"

    spacing pwell pwell 9 touching_ok \
	"P-Well spacing must be at least 9 (MOSIS rule #1.2)"

    width nwell 10 \
	"N-Well width must be at least 10 (MOSIS rule #1.1)"

    spacing nwell nwell 9 touching_ok \
	"N-Well spacing must be at least 9 (MOSIS rule #1.2)"

/* rule 1.3 is not checked */
/* for digital ckts where wells are                             *
 * not explicitly put, auto-generation may not ensure the min.  *
 * spacing and width rule: this is true when two geometries are *
 * diag.                                                        */
/* note: when both pwell and nwell are submitted they cannot overlap *
 * this is assured with the compose section - painting one well over  *
 * another will erase the old well                                   */
/* --------------------------------------------------------------- */

    width allDiff 3 \
	"Diffusion width must be at least 3 (MOSIS rule #2.1)"
    width psc/act,nsc/act,psd,nsd,clc/act,col 4 \
	"Ohmic Diffusion width must be at least 4 to assure the well width (MOSIS rule #1.1)"
    spacing allDiff allDiff 3 touching_ok \
	"Diffusion spacing must be at least 3 (MOSIS rule #2.2)"

    spacing allNDiff allPDiff 10 touching_illegal \
	"P-type diffusion must be 10 away from N-type diffusion (MOSIS rule #2.3)"
/* 2.4 */
    spacing allNDiff nsc/act,nnd 8 touching_illegal \
	"N-type diffusion must be 8 away from N-substrate contact (MOSIS rule #2.4)"
    spacing allPDiff psc/act,ppd 8 touching_illegal \
	"P-type diffusion must be 8 away from P-substrate contact (MOSIS rule #2.4)"
    spacing nncont/active,nnd	ppcont/active,ppd  6  touching_illegal \
	"Opposite well contacts must be separated by 6 (MOSIS rule #2.4)"

/* -------------------------------------------------------------- */
/* extra rules for explicit well * */

    spacing pdiff,pfet,epfet,pdc	pwell	5	touching_illegal \
	"P-diffusion and P-well must be separated by 5 (MOSIS rule #2.3)"
    spacing nncont/act,nsd	pwell	3	touching_illegal \
	"N-substrate diffusion and P-well must be separated by 3 (MOSIS rule #2.4)"
    spacing ndiff,nfet,enfet,ndc/active nwell	5	touching_illegal \
	"N-diffusion and N-well must be separated by 5 (MOSIS rule #2.3)"
    spacing ppcont/act,psd	nwell	3	touching_illegal \
	"P-substrate diffusion and N-well must be separated by 3 (MOSIS rule #2.4)"

/* --------------------------------------------------------------- */
/* Extra transistor rules */

    edge4way	poly,pc/active	pfet	3	pfet 0 0 \
	"Transistors must be at least 3 units wide (MOSIS rule #2)"
    edge4way	poly,pc/active	nfet	3	nfet 0 0 \
	"Transistors must be at least 3 units wide (MOSIS rule #2)"

/* --------------------------------------------------------------- */
/* 3.1 & 3.2 */
    width allPoly 2 \
	"Polysilicon width must be at least 2 (MOSIS rule #3.1)"
    spacing allPoly allPoly 2 touching_ok \
	"Polysilicon spacing must be at least 2 (MOSIS rule #3.2)"
/* 3.3 */
    edge4way nfet,pfet poly,pc/active  2  poly,pc/active poly,pc/active 2 \
	"Poly must overhang transistor by at least 2 (MOSIS rule #3.3)"
/* 3.4 */
    edge4way nfet,enfet ndiff,ndc/act 3 allNDiff ndiff,ndc/act 2 \
	"Diffusion must overhang transistor by at least 3 (MOSIS rule #3.4)"
    edge4way pfet,epfet pdiff,pdc/active 3 allPDiff pdiff,pdc/act 2 \
	"Diffusion must overhang transistor by at least 3 (MOSIS rule #3.4)"
    edge4way	nfet,pfet	space	1	poly	0	0 \
	"Transistor overhang is missing (MOSIS rule #3.4)"
    edge4way	enfet,epfet	space	1	poly2	0	0 \
	"Transistor overhang is missing"
/* 3.5 */
    edge4way pdiff,ndiff,pdc,ndc,ppd,nnd,nsc/act,psc/act poly2,ec,poly,pc 1 space 0 1 \
	"Poly and diffusion must be separated by at least 1 (MOSIS rule #3.5)"
    edge4way poly,pc,poly2,ec ndiff,pdiff,pdc,ndc,nsc/act,psc/act,nnd,ppd 1 space 0 1 \
	"Poly and diffusion must be separated by at least 1 (MOSIS rule #3.5)"
    edge  poly,pc,poly2,ec  space  1  space  space  1 \
	"Poly,poly2 and diffusion must be separated by at least 1 (MOSIS rule #3.5)"
    edge  ndiff,pdiff,pdc,ndc,nsc/act,psc/act,nnd,ppd space 1 space space 1 \
	"Poly,poly2 and diffusion must be separated by at least 1 (MOSIS rule #3.5)"
/* ---------------------------------------------------------------- */
/* 4.1 */
    spacing pfet,epfet	nncont/active,nnd	3	touching_illegal \
	"Transistors must be separated from substrate contacts by 3 (MOSIS rule #4.1.a)"
    spacing nfet,enfet	ppcont/active,ppd	3	touching_illegal \
	"Transistors must be separated from substrate contacts by 3 (MOSIS rule #4.1.b)"

    edge4way ppc/act,ppd space/active 4 ~(nfet,enfet)/act ppc/active,ppd,ndc,ndiff 3 \
	"Transistors must be separated from selects(generated by well cont) by 3 (MOSIS rule #4.1.c)"

    edge4way nnc/act,nnd space/active 4 ~(pfet,epfet)/act nnc/active,nnd,pdc,pdiff 3 \
	"Transistors must be separated from selects(generated by well cont) by 3 (MOSIS rule #4.1.c)"

    edge4way  ppc,ppd ~(ndiff,ndc,ppc,ppd)/active 4 ~(nfet,enfet)/act ~(ndiff,ndc,ppc,ppd)/active 4 \
        "Transistors must be separated from selects(generated by well cont) by 3 (MOSIS rule #4.1.e)"

    edge4way  nnc,nnd ~(pdiff,pdc,nnc,nnd)/active 4 ~(pfet,epfet)/act ~(pdiff,pdc,nnc,nnd)/active 4 \
        "Transistors must be separated from selects(generated by well cont) by \3 (MOSIS rule #4.1.f)"

/* 4.2 */
    edge4way ~(pdiff,pdc,pfet,epfet)/act pdiff,pdc,pfet 4 ~(nnc,nnd)/active \
    pdiff,pdc,pfet,epfet 2 \
	"Backedge of diffusion must be 4 from substrate diff (MOSIS rule #4.2)"
    edge4way ~(ndiff,ndc,enfet,nfet)/active ndiff,ndc,nfet 4 ~(ppc,ppd)/active \
    ndiff,ndc,nfet,enfet 2 \
	"Backedge of diffusion must be 4 from substrate diff (MOSIS rule #4.2)"
/* 4.2 is not checked in some cases - be careful */
/* 4.3 -- guaranteed automatically by CIF generator. */
/* 4.4 -- guaranteed automatically by CIF generator except diag. where
   this rule is not crucial */
/* --------------------------------------------------------------- */
/* 5.1 & 5.2 & 5.3*/
    width pc 4 \
	"Contact width must be at least 4 (MOSIS rule #5B.1,2,3)"
/* 5.4 & 5.5 --
 * Watch out here:  a spacing "touching_ok" rule CANNOT be used here:
 * it will miss certain checks.
 */
    edge4way  allPoly  ~(allPoly)/active  3  ~pc/active  ~(allPoly)/active  3 \
	"Poly contact must be at least 3 from other poly (MOSIS rule #5B.4,5)"
/* 5.6 --
 * This is mostly handled by 3.5 already, but need rule here to handle
 * case of pc abutting transistor.
 */
    spacing pc allDiff 1 touching_illegal \
	"Poly contact must be 1 unit from diffusion (MOSIS rule #5B.6)"
	
/* 5.7 -- not implemented */                                            
/* --------------------------------------------------------------- */
                                                                      
/* --------------------------------------------------------------- */
/* 6.1 & 6.2 & 6.3*/
    width ndc 4 \
	"N-Diffusion contact width must be at least 4 (MOSIS rule #6B.1,2,3)"
    width pdc 4 \
	"P-Diffusion contact width must be at least 4 (MOSIS rule #6B.1,2,3)"
    width ppcont/active 4 \
	"P-substrate contact width must be at least 4 (MOSIS rule #6B.1,2,3)"
    width nncont/active 4 \
	"N-substrate contact width must be at least 4 (MOSIS rule #6B.1,2,3)"
/* 6.4 & 6.5 --
 * Watch out here:  a spacing "touching_ok" rule CANNOT be used here:
 * it will miss certain checks.
 */
    edge4way  allDiff  ~(allDiff)/active  4  ~(ndc,pdc,nnc,ppc)/active \
	~(allDiff)/active  4 \
        "Diffusion contacts must be 4 from other diffusions (MOSIS rule #6B.4,5)"
/* 6.6 */
    spacing DiffCut pfet,nfet 1 touching_illegal \
	"Diffusion contacts cannot touch transistors (MOSIS rule #6B.6)"
/* 6.7 */
    spacing DiffCut poly,pc/act 1 touching_illegal \
	"Diffusion contact to field poly must be at least 1 (MOSIS rule #6B.7)"
/* 6.8 -- not implemented */
/* 6.9B :new rule-9/22/85 */
    spacing pc/act DiffCut 2 touching_illegal \
	"Poly contacts must be 2 away from diffusion contacts (MOSIS rule #6B.9)"
/* --------------------------------------------------------------- */
/* 7.1 & 7.2 */
    width allMetal1 3 \
	"First-level metal width must be at least 3 (MOSIS rule #7.1)"
    spacing allMetal1 allMetal1 3 touching_ok \
	"First-level metal spacing must be at least 3 (MOSIS rule #7.2)"
/* pad layer is bloated by 1 to create the m1 layer under it */
    spacing pad/metal1 allMetal1 4 touching_ok \
	"Pad to first-level metal spacing must be at least 4 (MOSIS rule #7.2)"
/* 7.3 & 7.4 */
    /* implied with 4x4 contacts */
/* ---------------------------------------------------------------- */
/* 8.1,2,3 */
    width m2c 4 \
	"Contact width must be at least 4 (MOSIS rule #8.1,2,3)"

/* for magic 4.10
    edge4way vh m2 1 m2 m2 1 \
	"Metal 2 must overhang Via Hole by 1"
    edge4way	vh/m2	space	1	m2	0	0 \
	"Metal 2 overhang VIA is missing"
*/
/* 8.4 & 8.5 */
    /* Vias have to be on flat surface */
    /* Don't allow poly or diffusion edges underneath metal2 contacts: */
    edge4way  allPoly  ~(allPoly)/active  1  ~m2c/metal2 \
	~(allPoly)/active  1 \
	"Via must be on a flat surface (MOSIS rule #8.4,5)" metal2
    edge4way  allDiff  ~(allDiff)/active  1  ~m2c/metal2 \
	~(allDiff)/active  1 \
	"Via must be on a flat surface (MOSIS rule #8.4,5)" metal2
    edge4way  ~(allPoly)/active allPoly  1  ~m2c/metal2  allPoly  1 \
	"Via must be on a flat surface (MOSIS rule #8.4,5)" metal2
    edge4way  ~(allDiff)/active  allDiff  1  ~m2c/metal2  allDiff  1 \
	"Via must be on a flat surface (MOSIS rule #8.4,5)" metal2
/* ---------------------------------------------------------------- */
/* 9.1,2 */
    width allMetal2 3 \
	"Second-level metal width must be at least 3 (MOSIS rule #9.1)"
    spacing allMetal2 allMetal2 4 touching_ok \
	"Second-level metal spacing must be at least 4 (MOSIS rule #9.2)"
/* 9.3 */
/* implied with via size of 4x4 */
/* --------------------------------------------------------------- */
/* 10.1 & 10.2 & 10.3 & 10.4 */
    /* these rules are not check because they are auto-generated */
    /* also becuse they are in absolute microns */
/* --------------------------------------------------------------- */
/* --------------------------------------------------------------- */
    /* Contacts must all be rectangular (no adjacent contacts
     * of same type).  This is handled using the corner checks
     * in the rules below.  Overlaps between contacts must be exact 
     * overlaps.  The only exception is overpad, which doesn't
     * matter.
     */
    edge4way 	m2c/m2	~m2c/m2	1	~m2c/m2 	(~m2c,m2c)/m2	1 \
	"Metal2 contacts must be rectangular (Magic rules)"
    edge4way 	ndc/m1	~ndc/m1	1	~ndc/m1		(~ndc,ndc)/m1	1 \
	"N-diffusion contacts must be rectangular (Magic rules)"
    edge4way 	pdc/m1	~pdc/m1	1	~pdc/m1		(~pdc,pdc)/m1	1 \
	"P-diffusion contacts must be rectangular (Magic rules)"
    edge4way 	pc/m1	~pc/m1	1	~pc/m1		(~pc,pc)/m1	1 \
	"Polysilicon contacts must be rectangular (Magic rules)"
    edge4way 	ppc/m1 ~ppc/m1 1 ~ppc/m1		(~ppc,ppc)/m1	1 \
	"P-substrate contacts must be rectangular (Magic rules)"
    edge4way 	nnc/m1	~nnc/m1	1	~nnc/m1		(~nnc,nnc)/m1	1 \
	"N-substrate contacts must be rectangular (Magic rules)"
/* new rules to make sure there are m2 under glass */
/* for magic 4.10
    edge4way    space glass 1 m2,m2c,vh 0 0 \
	"There must be metal 2 under the glass openning" metal2
*/

/* NEW 8/20/89 */

    edge4way 	emc/m1	~emc/m1	1	~emc/m1		(~emc,emc)/m1	1 \
	"Emitter contacts must be rectangular (Magic rules)"
    edge4way 	clc/m1	~clc/m1	1	~clc/m1		(~clc,clc)/m1	1 \
	"Collector contacts must be rectangular (Magic rules)"
    edge4way 	pbc/m1	~pbc/m1	1	~pbc/m1		(~pbc,pbc)/m1	1 \
	"P-base Contacts must be rectangular (Magic rules)"
    edge4way 	ec/m1	~ec/m1	1	~ec/m1		(~ec,ec)/m1	1 \
	"Electrode contacts must be rectangular (Magic rules)"
    edge4way 	cc/m1	~cc/m1	1	~cc/m1		(~cc,cc)/m1	1 \
	"Electrode contacts must be rectangular (Magic rules)"

/* -------------------------------------------------------------- */
/* electrode and capacator rules */
/* -------------------------------------------------------------- */
    width allPoly2 2 \
	"Electrode width must be at least 2 (EC #1)"
/*
    width nffet,pffet 2 \
	"Floating gate must be at lease 2 in length"
*/
    width ec 4 \
	"Electrode contact width must be at least 4 (EC #2)"
    width cc 4 \
	"Electrode contact width must be at least 4 (EC #2)"
    
    spacing poly2,ec,enfet,epfet,cc,cap cap,cc,enfet,epfet,poly2,ec 3 \
        touching_ok "Second-level poly spacing must be at least 3 (EC #3)"
    spacing allPoly2P allPolyP 2 touching_illegal \
	"Second-level poly to first level poly spacing must be at least 2 (EC #3)"
    spacing ec allDiff 1 touching_illegal \
	"Poly2 contact must be 1 unit from diffusion (MOSIS rule #5B.6)"
    spacing cap,cc allPoly 4 touching_ok \
	"Capacitor to poly spacing must be at least 4 (EC #4)"

    edge4way	poly2,ec/act	epfet	3	epfet 0 0 \
	"Transistors must be at least 3 units wide (MOSIS rule #2)"
    edge4way	poly2,ec/act	enfet	3	enfet 0 0 \
	"Transistors must be at least 3 units wide (MOSIS rule #2)"
    edge4way	cap,cc/act	pffet	3	pffet 0 0 \
	"Transistors must be at least 3 units wide (MOSIS rule #2)"
    edge4way	cap,cc/act	nffet	3	nffet 0 0 \
	"Transistors must be at least 3 units wide (MOSIS rule #2)"

/* the following two rules is installed because I cannot tell the
difference from the real cap or a floating gate's overlap */
/* you will get DRC violations if you are building Floating gates */
    edge4way  cap,cc space 1  poly poly 1 \
	"Cap must be overlaped by poly"
    edge4way  cap,cc poly 2  poly poly 2 \
	"Poly must be overlaped cap by 2"
    edge4way  cc/act space 1  cap cap 1 \
	"Cap contact must be overlaped by poly2"
    edge4way  cc/act space 1  poly poly 1 \
	"Cap contact must be overlaped by poly"
    edge4way  cc/act poly 4  poly poly 4 \
	"Cap contact must be overlaped by poly by 4"
    edge4way  cc/act cap 2  cap cap 2 \
	"Cap contact must be overlaped by electrode by 2"
    edge4way  cc/act poly 1  cap cap 1 \
	"Cap contact must be overlaped by electrode"
    edge4way  cap,cc poly 2  ~(pc)/act poly 2 \
	"Cap must be 2 away from polycontact" active

    edge4way  ec/act space 1  poly2 poly2 1 \
	"Electrode contact must be overlaped by poly2"
    edge4way  ec/act poly2 1  poly2 poly2 1 \
	"Electrode contact must be overlaped by poly2 by 1"

    edge4way  nw,pw  ~(nw,pw)/w  2  ~(cap,cc)/act ~(nw,pw)/w  2 \
	"Cap must be on a flat surface (rule #1)" active
    edge4way  ~(nw,pw)/w  nw,pw  2  ~(cap,cc)/act nw,pw  2 \
	"Cap must be on a flat surface (rule #1)" active
    edge4way  cap  ~(cap)/act  2  allFet,poly,poly2,space/act,cc/act \
	allDiff,poly 2 "Cap must be on a flat surface (rule #1)" active
/*
    edge4way  ~(allPoly)/act  allPoly  2  ~poly2/act ~(allPoly)/act  2 \
	"Electrode edge must be seperated by poly edge by 2 (rule #2)"
    edge4way  allPoly  ~(allPoly)/act  2  ~poly2/act allPoly  2 \
	"Electrode edge must be seperated by poly edge by 2 (rule #2)"
*/

    edge4way enfet,epfet poly2,ec/active  2  poly2,ec/act poly,pc/act 2 \
	"Poly2 must overhang transistor by at least 2 (MOSIS rule #3.3)"

    spacing DiffCut allPoly2 2 touching_illegal \
	"Diffusion contacts must be 2 away from electrode fet"

/* rules for NPN Bipolar stuff */

    width pbc,emc 4 \
	"Contact width must be at least 4 (MOSIS rule #5B.1,2,3)"
    spacing pbase,pbc/act clc/act 5 touching_illegal \
	"Pbase active must be 5 away from collector active contact"
    spacing pbase,pbc/act col 4 touching_illegal \
	"Pbase active must be 4 away from collector active"
    spacing pbc emc/act,emit 7 touching_illegal \
	"Pbase active must be 7 (4+2+1) away from emitter active"
    spacing emc/act,emit allNDiff,allPDiff,allOhmics 15 touching_illegal \
	"Emitter active must be 15 away from other actives"
    spacing pbc allNDiff,allPDiff,allOhmics 12 touching_illegal \
	"Pbase contact must be 12 away from other actives"
    edge4way  pbc/act space 1  pbase pbase 1 \
	"Base Contact must be overlaped by pbase"
    edge4way  pbc/act pbase 1  pbase pbase 1 \
	"Pbase must overlap base contact by 1"
    edge4way  emc/act,emit space 1  pbase pbase 1 \
	"Emitter contact must be overlaped by pbase"
    edge4way  emc/act,emit pbase 4  pbase pbase 4 \
	"Emitter contact must be overlaped by pbase by 4"

/* -------------------------------------------------------------- */
/* overlapping rules */ 
    exact_overlap m2c,ndc,pdc,pc,ppcont/m1,nncont/m1,ec,cc
    no_overlap	pfet,nfet	pfet,nfet
    no_overlap	epfet,enfet	epfet,enfet

end





